<%= render file: @cms_site.template_dir + '_header.html' %>

<% case @share_obj.class.to_s %>
  <% when 'Site' %>
    <%= render file: @cms_site.template_dir + '_share_site.html.erb' %>
  <% when 'User' %>
    <%= render file: @cms_site.template_dir + '_share_user.html.erb' %>
  <% when 'Community' %>
    <%= render file: @cms_site.template_dir + '_share_community.html.erb' %>
  <% else %>
    <%= render file: @cms_site.template_dir + '_share_default.html.erb' %>
<% end %>

<%= render file: @cms_site.template_dir + '_footer.html' %>
